@model IEnumerable<MobileStore.Models.GroupJoin>

@{ ViewData["Title"] = "Sales"; }

<div class="row">

    <div class="col-4">

    </div>


    <div class="col-4">
        <table class="table">
            <thead>
                <tr>
                    <th>
                        User Name
                    </th>
                    <th>
                        Total
                    </th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrice)$
        </td>
    </tr>}
                }
            </tbody>
        </table>


    </div>
</div>